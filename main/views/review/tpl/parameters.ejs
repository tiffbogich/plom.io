<% thetas.forEach(function(theta, i){ %>
   <div class="model-box">
     <ul>
       <li><a href="/<%= theta.username %>"><%= theta.username %></a></li>
       <li>DIC: <%= theta.dic %></li>
       <% if (_.last(theta.design.cmd).fit) { %>
       <li>fit: <%= _.last(theta.design.cmd).fit %></li>
       <% } %>
       <li><%= _.last(theta.design.cmd).algorithm %></li>
     </ul>

     <button class="btn review-theta" type="button" value="<%= i %>">review</button>
   </div>
<% }); %>
